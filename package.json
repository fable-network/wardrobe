{
  "name": "wardrobe",
  "description": "FashionTrade Component Library",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/fashiontrade/wardrobe",
  "repository": {
    "type": "git",
    "url": "https://github.com/fashiontrade/wardrobe.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "styleguidist server",
    "build": "grunt && styleguidist build",
    "test": "NODE_ENV=test jest --no-cache",
    "snapshots": "jest --updateSnapshot",
    "generate-tests": "node generateTests.js && jest --updateSnapshot && jest"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.6",
    "downshift": "^2.0.16",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.2",
    "node-sass": "^4.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-styleguidist": "^7.1.3",
    "sass-loader": "^7.0.3",
    "styled-components": "^3.4.2",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.4.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-dom-munger": "^3.4.0",
    "grunt-svgstore": "^1.0.0",
    "prettier": "1.13.7"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/test-setup.js"
  }
}
