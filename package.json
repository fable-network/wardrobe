{
  "name": "@fashiontrade/wardrobe",
  "description": "FashionTrade Component Library",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/fashiontrade/wardrobe",
  "repository": {
    "type": "git",
    "url": "https://github.com/fashiontrade/wardrobe.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "pre-commit": [
    "lint",
    "test",
    "add-dist"
  ],
  "dependencies": {},
  "scripts": {
    "start": "styleguidist server",
    "prepublishOnly": "npm run lint && npm run test && npm run dist",
    "lint": "eslint src scripts tools",
    "test": "NODE_ENV=test jest --no-cache",
    "dist": "rm -rf ./dist && grunt && webpack --mode=production --config webpack.production.config.js",
    "add-dist": "./add-dist.sh",
    "build": "styleguidist build",
    "update-snapshots": "jest --updateSnapshot",
    "generate-tests": "node scripts/generateTests.js && jest",
    "generate-component": "node scripts/generateComponent.js"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.4",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "classnames": "2.2.6",
    "downshift": "2.0.16",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-mocha-no-only": "1.0.0",
    "eslint-plugin-react": "7.10.0",
    "grunt": "1.0.3",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-svgmin": "5.0.0",
    "grunt-svgstore": "1.0.0",
    "jest": "23.4.2",
    "node-sass": "4.9.2",
    "pre-commit": "1.2.2",
    "prettier": "1.13.7",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-styleguidist": "7.2.3",
    "sass-loader": "7.0.3",
    "styled-components": "3.3.3",
    "svg-inline-loader": "0.8.0",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/test-setup.js"
  }
}
