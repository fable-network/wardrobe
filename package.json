{
  "name": "@fashiontrade/wardrobe",
  "description": "FashionTrade Component Library",
  "version": "5.1.0",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/fashiontrade/wardrobe",
  "repository": {
    "type": "git",
    "url": "https://github.com/fashiontrade/wardrobe.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4",
    "yarn": "^1.6.0"
  },
  "pre-commit": [
    "lint-staged",
    "lint",
    "test",
    "add-dist"
  ],
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config webpack.analyzer.config.js",
    "start": "styleguidist server",
    "prepublishOnly": "yarn lint && yarn test && yarn dist",
    "lint": "eslint src scripts tools",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=test jest --no-cache",
    "dist": "rm -rf ./dist && grunt && webpack --mode=production --config webpack.production.config.js",
    "add-dist": "./add-dist.sh",
    "build": "styleguidist build",
    "update-snapshots": "jest --updateSnapshot",
    "generate-tests": "node scripts/generateTests.js && jest",
    "generate-component": "chmod +x scripts/generate-component.sh && scripts/generate-component.sh"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "a11y-focus-scope": "1.1.3",
    "a11y-focus-store": "1.0.0",
    "classnames": "2.2.6",
    "lodash.debounce": "4.0.8",
    "lodash.get": "4.4.2",
    "prop-types": "15.6.1",
    "tabbable": "3.1.1"
  },
  "peerDependencies": {
    "highcharts": "^7.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@fashiontrade/eslint-config-ft-react-app": "0.5.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.4",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "colors": "1.3.1",
    "copy-webpack-plugin": "4.5.2",
    "downshift": "2.0.16",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-mocha-no-only": "1.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.10.0",
    "grunt": "1.0.3",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-svgmin": "5.0.0",
    "grunt-svgstore": "2.0.0",
    "highcharts": "7.0.3",
    "jest": "23.6.0",
    "jsdom": "^12.1.0",
    "license-info-webpack-plugin": "1.0.2",
    "license-webpack-plugin": "2.0.1",
    "lint-staged": "8.1.5",
    "lodash": "4.17.11",
    "node-sass": "4.9.2",
    "pre-commit": "1.2.2",
    "prettier": "1.13.7",
    "react": "16.2.0",
    "react-dom": "16.2.1",
    "react-styleguidist": "8.0.6",
    "sass-loader": "7.0.3",
    "styled-components": "3.3.3",
    "svg-inline-loader": "0.8.0",
    "svg4everybody": "2.1.9",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "2.0.12",
    "webpack-node-externals": "1.7.2",
    "what-input": "5.1.3"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/test-setup.js"
  },
  "lint-staged": {
    "*.{js,scss,md}": [
      "prettier --config package.json --write",
      "git add"
    ]
  }
}
